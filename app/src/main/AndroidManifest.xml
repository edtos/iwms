<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".viewmodel.IWMSApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ssh9"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".view.Menu1Activity"
            android:exported="false" />
        <activity
            android:name=".view.EnvironmentListActivity"
            android:exported="false"
            android:theme="@style/Theme.Ssh9" />
        <activity
            android:name=".view.HomeActivity"
            android:exported="false"
            android:theme="@style/Theme.Ssh9" />
        <activity
            android:name=".view.SplashScreenActivity"
            android:exported="true"
            android:theme="@style/Theme.Ssh9">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.TermsConditionsActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Ssh9"></activity>
        <activity
            android:name=".view.LoginActivity"
            android:exported="false"
            android:label="@string/app_name"
            android:theme="@style/Theme.Ssh9" />
        <activity
            android:name=".view.AddEditEnvironmentActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Ssh9" />
    </application>

</manifest>